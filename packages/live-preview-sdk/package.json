{
  "name": "@contentful/live-preview",
  "version": "2.12.5",
  "author": "Contentful GmbH",
  "license": "MIT",
  "description": "Preview SDK for both the field tagging connection + live content updates",
  "source": "./src/index.tsx",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "react",
    "style.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "require": "./dist/react.cjs",
      "types": "./dist/react.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/live-preview.git"
  },
  "bugs": {
    "url": "https://github.com/contentful/live-preview/issues"
  },
  "scripts": {
    "start": "vite build --watch",
    "build": "vite build",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "coverage": "vitest run --coverage",
    "prepublish": "yarn build"
  },
  "homepage": "https://github.com/contentful/live-preview#readme",
  "dependencies": {
    "@contentful/rich-text-types": "^16.2.0",
    "@contentful/visual-sdk": "^1.0.0-alpha.34",
    "graphql-tag": "^2.12.6",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/react": "14.1.2",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^20.1.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "contentful": "^10.3.4",
    "contentful-management": "^11.0.1",
    "graphql": "^16.6.0",
    "jsdom": "^23.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "type-fest": "^4.0.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.2",
    "vite-plugin-dts": "2.3.0",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
