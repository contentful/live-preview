{
  "name": "@contentful/live-preview",
  "version": "4.9.5",
  "author": "Contentful GmbH",
  "license": "MIT",
  "description": "Preview SDK for both the field tagging connection + live content updates",
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "react"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/live-preview.git"
  },
  "bugs": {
    "url": "https://github.com/contentful/live-preview/issues"
  },
  "scripts": {
    "start": "vite build --watch",
    "build": "vite build",
    "benchmark": "vitest bench",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "coverage": "vitest run --coverage",
    "prepublish": "yarn build"
  },
  "homepage": "https://github.com/contentful/live-preview#readme",
  "dependencies": {
    "@contentful/content-source-maps": "^0.12.1",
    "@contentful/rich-text-types": "^17.0.0",
    "flatted": "^3.2.9",
    "graphql-tag": "^2.12.6",
    "json-pointer": "^0.6.2",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/react": "15.0.7",
    "@types/json-pointer": "1.0.34",
    "@types/lodash.isequal": "4.5.8",
    "@types/node": "25.1.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "4.2.2",
    "contentful": "11.9.0",
    "contentful-management": "11.69.1",
    "graphql": "16.12.0",
    "jsdom": "27.4.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "type-fest": "5.4.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  }
}
